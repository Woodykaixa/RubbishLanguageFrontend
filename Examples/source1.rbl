@import
func void WriteLine(str s);

str greeting = "Hello world!\nI'm running on CLR!";


func i64 fib(i64 n) {
	if (n == 1) {
		return 1;
	}
	return n*fib(n-1);
}

func i64 foo(str s) {
	WriteLine(s);
	i64 isNull = 10 + not address_of s;
	loop (isNull) {
		isNull = isNull - 1;
	}
	return fib(10);
}

foo(greeting);